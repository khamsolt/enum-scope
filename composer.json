{
    "name": "khamsolt/enum-scope",
    "description": "Enum Shared Code",
    "keywords": [
        "khamsolt",
        "enum-scope"
    ],
    "homepage": "https://github.com/khamsolt/enum-scope",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Khamidov",
            "email": "khamidov.che@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Khamsolt\\EnumScope\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Khamsolt\\EnumScope\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
